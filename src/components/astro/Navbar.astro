---
//@ts-ignore
import Navbar from "../react/Navbar";
import { directusAssets, globalFetching, navbarFetching } from "../../services/api";
const navbarResData = await navbarFetching();

const navbarData = navbarResData.map((data) => {
  const newData = {
    ...data,
    // header_image: data?.header_image ? directusAssets(data?.header_image): ''
  };
  
  return newData;
})

const globalData = await globalFetching();
globalData.logo = directusAssets(globalData.logo);

---

<Navbar data={navbarData} global={globalData} client:only="react"/>


